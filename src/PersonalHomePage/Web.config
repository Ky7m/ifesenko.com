        <add key="UnobtrusiveJavaScriptEnabled" value="true" />
        <add key="CdnUrl" value="//ifesenko.azureedge.net" />
        <add key="emailService:Account" value="ifesenko.com" />
        <add key="emailService:Password" value="OhQ0mXBKovVnjK9tJwr1" />

@using System.Configuration
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Igor Fesenko - Personal Site. I'm available for helping your team succeed. You can find details on the home page. Please feel free to contact me.">
    <meta name="keywords" content="Web Applications,Sites,Web Services,SOA,Security,Identity,Data Access,Integration,Databases,Desktop Applications,Cloud Computing,Mobile Client Applications,Big Data">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta http-equiv='x-dns-prefetch-control' content='on'>
    <link rel="shortcut icon" href="@CdnUrl.Content("favicon.ico")" type="image/x-icon">
    <link rel="icon" href="@CdnUrl.Content("favicon.ico")" type="image/x-icon">
    <link rel="apple-touch-icon" href="@CdnUrl.Content("apple-touch-icon.png")">
    <title>Igor Fesenko - You're welcome!</title>

    @Styles.Render("~/bundles/font-awesome")
    @Styles.Render("~/bundles/css")

    @AnalyticsHelper.GetGoogleAnalyticsScript()
    @AnalyticsHelper.GetAppInsightsScript()
    @AnalyticsHelper.GetYandexMetricsScript()
    <script async type="text/javascript">
        window.options={cdnUrl:'@ConfigurationManager.AppSettings["CdnUrl"]', enableOptimizations: @BundleTable.EnableOptimizations.ToString().ToLowerInvariant()};
    </script>
</head>

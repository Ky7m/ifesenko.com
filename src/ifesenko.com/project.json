{
  "webroot": "wwwroot",
  "version": "5.0.5",
  "compilationOptions": {
    "warningsAsErrors": true,
    "emitEntryPoint": true
  },
    "dependencies": {
        "Microsoft.AspNet.Mvc": "6.0.0-rc1-*",
        "Microsoft.AspNet.Mvc.TagHelpers": "6.0.0-rc1-*",
        "Microsoft.AspNet.Diagnostics": "1.0.0-rc1-*",
        "Microsoft.AspNet.Server.Kestrel": "1.0.0-rc1-*",
        "Microsoft.AspNet.StaticFiles": "1.0.0-rc1-*",
        "Microsoft.AspNet.Tooling.Razor": "1.0.0-rc1-*",
        "Microsoft.AspNet.IISPlatformHandler": "1.0.0-rc1-*",
        "Microsoft.Extensions.Configuration.Json": "1.0.0-rc1-*",
        "Microsoft.Extensions.Logging": "1.0.0-rc1-*",
        "Microsoft.Extensions.Logging.Console": "1.0.0-rc1-*",
        "Microsoft.Extensions.Logging.Debug": "1.0.0-rc1-*",
        "Microsoft.Extensions.Configuration.EnvironmentVariables": "1.0.0-rc1-*",
        "Microsoft.VisualStudio.Web.BrowserLink.Loader": "14.0.0-rc1-*",
        "Microsoft.IO.RecyclableMemoryStream": "1.1.0",
        "Newtonsoft.Json": "8.0.3",
        "NWebsec.Mvc": "5.0.0-*",
        "StackExchange.Redis": "1.1.572-*",
        "WebMarkupMin.Core": "2.0.0-rc7",
        "WebMarkupMin.AspNet5": "2.0.0-*",
        "Wire": "0.0.6",
        "WindowsAzure.Storage": "7.0.1-*",
        "Microsoft.ApplicationInsights.AspNet": "1.0.0-rc1-*"
    },
  "commands": {
    "web": "Microsoft.AspNet.Server.Kestrel"
  },

  "frameworks": {
    "dnx451": {
      "dependencies": {
        "System.Net.Http": "4.0.1-beta-23516"
      }
    }
  },

  "exclude": [
    "wwwroot",
    "node_modules",
    "bower_components",
    "typings"
  ],
  "publishExclude": [
    "node_modules",
    "bower_components",
    "typings",
    "**.xproj",
    "**.user",
    "**.vspscc"
  ],
  "scripts": {
    "prepublish": [ "npm install", "bower install", "gulp build" ]
  }
}

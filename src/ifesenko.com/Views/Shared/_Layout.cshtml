@using Microsoft.AspNet.Hosting
@inject Microsoft.AspNet.Hosting.IHostingEnvironment HostingEnvironment
@inject Microsoft.Extensions.PlatformAbstractions.IApplicationEnvironment ApplicationEnvironment
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Igor Fesenko - Personal Site. I'm available for helping your team succeed. You can find details on the home page. Please feel free to contact me.">
    <meta name="keywords" content="Web Applications,Sites,Web Services,SOA,Security,Identity,Data Access,Integration,Databases,Desktop Applications,Cloud Computing,Mobile Client Applications,Big Data">
    <meta name="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>@ViewData["Title"]</title>
    @* <link rel="dns-prefetch" href="@ConfigurationManager.AppSettings["CdnUrl"]">*@
    <link rel="dns-prefetch" href="//cdnjs.cloudflare.com">
    <link rel="dns-prefetch" href="//fonts.gstatic.com">
    <link rel="dns-prefetch" href="//az416426.vo.msecnd.net">
    <link rel="dns-prefetch" href="//dc.services.visualstudio.com">
    @*<link rel="shortcut icon" href="@CdnUrl.Content("favicon.ico")" type="image/x-icon">
        <link rel="icon" href="@CdnUrl.Content("favicon.ico")" type="image/x-icon">
        <link rel="apple-touch-icon" href="@CdnUrl.Content("apple-touch-icon.png")">*@
    <environment names="@nameof(EnvironmentName.Development)">
        <link asp-append-version="true" rel="stylesheet" href="~/css/font-awesome.css" />
        <link asp-append-version="true" rel="stylesheet" href="~/css/bootstrap.css" />
        <link asp-append-version="true" rel="stylesheet" href="~/css/app.css" />
    </environment>
    <environment names="@nameof(EnvironmentName.Staging),@nameof(EnvironmentName.Production)">
        <link rel="stylesheet" href="https://ajax.aspnetcdn.com/ajax/bootstrap/3.3.5/css/bootstrap.min.css" />
        <link asp-append-version="true" rel="stylesheet" href="~/css/app.css" />
    </environment>
    <partial name="_AnalyticsHead" />
</head>
<body data-spy="scroll" data-target=".navbar-custom" data-offset="50">
    @RenderSection("PreContent", required: false)
    @RenderBody()
    <footer id="footer" role="contentinfo">
        <div class="container">
            <div class="row">
                <p class="copy text-center">
                    @*@BundleConfig.Version*@
                </p>
            </div>
        </div>
    </footer>

    @RenderSection("PostContent", required: false)

    <environment names="@nameof(EnvironmentName.Development)">
        <script asp-append-version="true" src="~/js/jquery.js"></script>
        <script asp-append-version="true" src="~/js/bootstrap.js"></script>
        <script asp-append-version="true" src="~/js/app.js"></script>
    </environment>
    <environment names="@nameof(EnvironmentName.Staging),@nameof(EnvironmentName.Production)">
        <script src=""></script>
    </environment>


    @RenderSection("scripts", required: false)
</body>
</html>
<!-- Environment: @HostingEnvironment.EnvironmentName -->
<!-- Framework: @ApplicationEnvironment.RuntimeFramework -->